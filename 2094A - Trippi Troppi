#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

void slove()
{
    string a, b, c;
    cin >> a >> b >> c;

    cout << a[0] << b[0] << c[0] <<endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    int t;
    cin >> t;

    while(t--)
        slove();

    return 0;
}
