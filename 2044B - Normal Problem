#include <bits/stdc++.h>
using namespace std;

void solve() {
    string a;
    cin >> a;

    reverse(a.begin(), a.end());

    for (int i = 0; i < a.size(); i++) {
        if (a[i] == 'p') cout << 'q';
        else if (a[i] == 'q') cout << 'p';
        else cout << 'w';
    }
    cout << '\n';
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;

    while (t--) solve();

    return 0;
}
