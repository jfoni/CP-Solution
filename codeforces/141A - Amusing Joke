#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
void slove()
{
    string g, h, nw;
    cin >> g >> h >> nw;
 
    string nw1 = g + h;
 
    sort(nw.begin(), nw.end());
    sort(nw1.begin(), nw1.end());
 
    if(nw == nw1) cout << "YES" << endl;
    else cout << "NO" << endl;
}
 
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
 
    slove();
    return 0;
}
