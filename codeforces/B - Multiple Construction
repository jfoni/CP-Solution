#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
int main(){
    int t; cin >> t;
    while(t--){
        int n; cin >> n;
        for(int i=n; i>=1; i--){
            cout << i << " ";
        }
        cout << n << " ";
        for(int i=1; i<n; i++){
        cout << i << " ";
        }
        cout << endl;
 
    }
 
    return 0;
}
