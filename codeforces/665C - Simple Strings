#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    string s;
    cin >> s;

    for(int i = 0; i < s.size() - 1; i++){
        if(s[i] == s[i+1]){
            for(char c = 'a'; c <= 'z'; c++){
                if(c != s[i] && (i + 2 >= s.size() || c != s[i + 2])){
                    s[i + 1] = c;
                    break;
                }
            }
        }
    }

    cout << s <<endl;
}
