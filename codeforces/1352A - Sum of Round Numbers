#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int t;
    cin >> t;

    while(t--)
    {
        string n;
        cin >> n;

        int count = 0;

        for(int i = 0; i < n.size(); i++)
        {
            if(n[i] != '0')
            {
                count ++;
            }
        }

        cout << count <<endl;

        for(int i = 0; i < n.size(); i++)
        {
            if(n[i] != '0')
            {
                int value = (n[i] - '0') * pow(10, n.size() - i - 1);
                cout << value << " ";
            }
        }
    }
}

