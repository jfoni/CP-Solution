#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int t;
    cin >> t;

    while(t--)
    {
        ll n;
        cin >> n;

        if(n % 2 == 1 || n < 4) cout << -1 <<endl;

        else {
            ll mn = (n + 5) / 6;
            ll mx = n / 4;

            cout << mn << " " << mx <<endl;
        }
    }
}
