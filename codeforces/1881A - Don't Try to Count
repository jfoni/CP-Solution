#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

void slove()
{
    int n, m;
    cin >> n >> m;

    string x, s;
    cin >> x;
    cin >> s;

    for(int i = 0; i <= 5; i++)
    {
        int F = x.find(s);
        if(F < x.size())
        {
            cout << i <<endl;
            return;
        }

        x += x;
    }

    cout << -1 <<endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    int t;
    cin >> t;

    while(t--)
    {
        slove();
    }

    return 0;
}
