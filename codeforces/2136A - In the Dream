#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
 
void solve() {
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    if(c >= a && d >= b) {
        int x1 = a, y1 = b;
        int x2 = c - a, y2 = d - b;
        if(x1 <= 2*(y1+1) && y1 <= 2*(x1+1) && x2 <= 2*(y2+1) && y2 <= 2*(x2+1))
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    } else {
        cout << "NO" << endl;
    }
}
 
int main() {
    int t;
    cin >> t;
    while(t--) solve();
    return 0;
}
