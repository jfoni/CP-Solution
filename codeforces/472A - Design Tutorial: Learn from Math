#include <iostream>
using namespace std;
typedef long long ll;

void slove()
{
    int n;
    cin >> n;

    if(n % 2 == 0) {

        cout << 4 << " " << n-4 << endl;
    } else {

        cout << 9 << " " << n-9 << endl;
    }
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    slove();
    return 0;

    return 0;
}

