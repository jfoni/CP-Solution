#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

void slove()
{
    int a, b, c;
    cin >> a >> b >> c;

    int arr[3] = {a,b,c};

    sort(arr, arr + 3);

    cout << arr[1] <<endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    int t;
    cin >> t;

    while(t--)
        slove();
    return 0;
}
