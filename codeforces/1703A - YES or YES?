#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int t;
    cin >> t;

    while(t--)
    {
        string s;
        cin >> s;
        bool found = false;
        for(int i=0; i<s.size(); i++)
        {
            s[i]=tolower(s[i]);
        }


        if(s=="yes")
            cout << "YES" <<endl;

        else
            cout << "NO" <<endl;
    }
}
