#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int t;
    cin >> t;
    while(t--){
        string s;
        cin >> s;

        int firstsum = (s[0] - '0')+(s[1] - '0')+(s[2] - '0');
        int lastsum =  (s[3] - '0')+(s[4] - '0')+(s[5] - '0');

        if(firstsum == lastsum){
            cout << "YES" <<endl;
        }
        else{
            cout <<"NO" <<endl;
        }
    }
}
