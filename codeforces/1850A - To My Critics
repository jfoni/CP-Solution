#include <iostream>
using namespace std;
typedef long long ll;

void slove()
{
    int a, b, c;
    cin >> a >> b >> c;

    if(a + b >= 10 || a + c >= 10 || b + c >= 10)
        cout << "YES" << endl;
    else
        cout << "NO" << endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    int t;
    cin >> t;
    while(t--){
        slove();
    }

    return 0;
}



