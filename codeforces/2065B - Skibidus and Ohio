#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int t;
    cin >> t;

    while(t--)
    {

        string s;
        cin >> s;

        bool f=false;

        for(int i=0;i<s.size();i++)
        {
            if(s[i]==s[i+1])
            {
                f=true;
                break;
            }
        }

    if(f)
        cout << 1 << endl;
    else
        cout << s.size() << endl;
    }

}




