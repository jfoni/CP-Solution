#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int t;
    cin >> t;

    while(t--)
    {
        set<int>s;

        for(int i=0; i<4; i++)
        {
            int num;
            cin >> num;
            s.insert(num);
        }
        if(s.size()==1)
        {
            cout <<"YES" <<endl;
        }
        else
        {
            cout <<"NO" <<endl;
        }
    }
}
