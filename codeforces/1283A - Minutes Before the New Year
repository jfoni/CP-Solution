#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int t;
    cin >> t;

    while(t--){
        int h, m;
        cin >> h >> m;

        int total_Min  = 24 * 60;
        int passed_Min = h * 60 + m;
        int re_Min = total_Min - passed_Min;

        cout << re_Min <<endl;
    }
}
