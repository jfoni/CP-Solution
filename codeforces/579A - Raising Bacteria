#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void slove()
{
    int n;
    cin >> n;
    int count = 0;
    while(n) {
        count += n & 1;
        n >>= 1;
    }
    cout << count << endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    slove();
    return 0;
}

