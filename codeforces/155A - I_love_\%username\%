#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve()
{
    int n;
    cin >> n;

    vector<int> v(n);

    for(int i = 0; i < n; i++) cin >> v[i];

    int count = 0;

    int max_score = v[0];
    int min_score = v[0];

    for(int i = 1; i < n; i++){
        if(v[i] > max_score){
            count++;
            max_score = v[i];
        }

        else if (v[i] < min_score){
            count++;
            min_score = v[i];
        }
    }

    cout << count <<endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    solve();

    return 0;
}

