#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
 
void solve(){
    int n, m, i, j;
    cin >> n >> m >> i >> j;
 
    cout << 1 << " " << 1 << " " << n << " " << m << "\n";
}
     
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
    int t;
    cin >> t;
    while(t--){
        solve();
    }
    return 0;
}
