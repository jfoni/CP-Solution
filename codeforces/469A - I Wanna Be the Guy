#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
    int n;
    cin >> n;

    set<int> passed;

    int p;
    cin >> p;

    for(int i=0;i<p;i++){
        int x;
        cin >> x;
        passed.insert(x);
    }

    int q;
    cin >> q;

    for(int i=0;i<q;i++){
        int x;
        cin >> x;
        passed.insert(x);
    }

    if((int)passed.size()==n){
        cout << "I become the guy." <<endl;
    }
    else
        cout << "Oh, my keyboard!" <<endl;
}
