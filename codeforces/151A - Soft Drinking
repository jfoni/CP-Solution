#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

void solve()
{
    int n, k, l, c, d, p, nl, np;
    cin >> n >> k >> l >> c >> d >> p >> nl >> np;

    int hm = k * l;
    int toa = hm / nl;
    int lim = c * d;
    int sa = p / np;

    int mn = min({toa, lim, sa}) / n;

    cout << mn << endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    solve();

    return 0;
}


